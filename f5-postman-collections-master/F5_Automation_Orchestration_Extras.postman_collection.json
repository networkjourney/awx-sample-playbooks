						"url": "https://{{iworkflow_mgmt}}/mgmt/cm/system/licensing/pool/initial-activation",
						"method": "POST",
						"header": [
							{
								"key": "X-F5-Auth-Token",
								"value": "{{iworkflow_auth_token}}",
								"description": ""
